{
  "name": "@exdatic/flowise-embed",
  "version": "1.2.0-2",
  "description": "Javascript library to display flowise chatbot on your website",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup --environment NODE_ENV:development --watch --config rollup.config.js",
    "build": "rollup --environment NODE_ENV:production --config rollup.config.js",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint-fix": "eslint --fix \"src/**/*.ts*\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@ts-stack/markdown": "^1.5.0",
    "flowbite": "^2.3.0",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.7.4",
    "solid-element": "1.8.0",
    "solid-js": "1.8.15"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.23.3",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.15.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "10.4.17",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-solid": "1.8.12",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-solid": "0.13.0",
    "husky": "^8.0.3",
    "postcss": "8.4.33",
    "prettier": "^3.2.4",
    "react": "18.2.0",
    "rollup": "4.9.6",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-serve": "3.0.0",
    "rollup-plugin-typescript-paths": "1.5.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exdatic/flowise-embed.git"
  }
}
